<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.namnp.androidjetpack" >

    <application
        android:name=".AndroidJetpackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidJetpack" >
        <activity
            android:name=".di.DependencyInjectionActivity"
            android:exported="false" />
        <activity
            android:name=".work_manager.WorkManagerActivity"
            android:exported="false" />
        <activity
            android:name=".room.RoomActivity"
            android:exported="false" />
        <activity
            android:name=".coroutines.CoroutineActivity"
            android:exported="false" />
        <activity
            android:name=".navigation.NavigationActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view_model.ViewModelActivity"
            android:exported="false" />
        <activity
            android:name=".data_binding.DataBindingActivity"
            android:exported="true" />
        <activity
            android:name=".data_binding.TwoWayBindingActivity"
            android:exported="true" />
    </application>

</manifest>